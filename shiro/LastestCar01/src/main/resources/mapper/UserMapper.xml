<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.lastestcar01.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.example.lastestcar01.domain.User">
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="amount" column="amount" jdbcType="DECIMAL"/>
            <result property="favoriteCars" column="favorite_cars" jdbcType="VARCHAR"/>
            <result property="comparedCars" column="compared_cars" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        username,password,amount,
        favorite_cars,compared_cars
    </sql>
</mapper>
